{
    "name": "Roblox Custom",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://*.roblox.com/*"],
            "js": ["content.js"],
            "css": ["content.css"],
            "run_at": "document_idle",
            "all_frames": true
        }
    ],
    "permissions": [
        "tabs",
        "storage"
    ]
}